<Window x:Class="Selenium.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Selenium"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="Start" Content="Button" HorizontalAlignment="Left" Margin="519,60,0,0" VerticalAlignment="Top" Width="75" Click="Start_Click"/>
        <TextBox x:Name="LOG" HorizontalAlignment="Left" Height="327" Margin="50,60,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="338"/>
        <CheckBox Content="CheckBox" HorizontalAlignment="Left" Margin="519,159,0,0" VerticalAlignment="Top"/>
        <RadioButton x:Name="rb" Content="RadioButton" HorizontalAlignment="Left" Margin="525,186,0,0" VerticalAlignment="Top" Checked="RadioButton_Checked" IsChecked="True"/>
        <Button x:Name="btn2" Content="Button" HorizontalAlignment="Left" Margin="519,102,0,0" VerticalAlignment="Top" Width="75" Click="Btn2_Click"/>

    </Grid>
</Window>
